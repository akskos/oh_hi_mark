{
    "name": "Oh, hi mark",
    "version": "0.0.1",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "permissions": ["storage"],
    "browser_action": {
	"default_popup": "hello.html"
    },
    "commands": {
	"_execute_browser_action": {
	    "suggested_key": {
		"default": "Ctrl+Shift+F",
		"mac": "MacCtrl+Shift+F"
	    },
	    "description": "Opens hello.html"
	}
    },
    "content_scripts": [{
	"matches": ["*://*/*"],
	"js": ["jquery-3.3.1.min.js", "script.js"],
	"css": ["style.css"],
	"run_at": "document_start"
    }],
    "web_accessible_resources": ["jesse.small.png", "drinking.gif", "zucc.small.png", "eyes.small.png", "water.small.png", "rofl.small.png", "thicc.small.png", "dad.small.png", "loveyou.small.png"] 
}
